<Window x:Class="SelfExam.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SelfExam"
        mc:Ignorable="d"
        Title="MainWindow" Height="170" Width="250" ResizeMode="NoResize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Name="CreateButton" Content="새 시험 생성" Grid.Row="0" Grid.Column="0" Margin="15,15,15,15" Click="CreateButton_Click"/>
        <Button Name="OpenButton" Content="시험 열기" Grid.Row="0" Grid.Column="1" Margin="15,15,15,15" Click="OpenButton_Click"/>
        <Button Name="EditButton" Content="시험 편집" Grid.Row="1" Grid.Column="0" Margin="15,15,15,15" Click="EditButton_Click"/>
        <Button Name="SettingButton" Content="기타 설정" Grid.Row="1" Grid.Column="1" Margin="15,15,15,15" Click="SettingButton_Click"/>
    </Grid>
</Window>
